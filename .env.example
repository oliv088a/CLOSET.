# App
APP_NAME=Closet
APP_ENV=development
APP_VERSION=1.0.0

# API
API_URL=http://localhost:3000
API_TIMEOUT=30000

# API Keys
WEATHERSTACK_API_KEY=your_weatherstack_api_key
GOOGLE_GEMINI_API_KEY=your_gemini_api_key
REMOVE_BG_API_KEY=your_remove_bg_api_key

# Firebase Configuration
EXPO_PUBLIC_FIREBASE_API_KEY=
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=
EXPO_PUBLIC_FIREBASE_PROJECT_ID=
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
EXPO_PUBLIC_FIREBASE_APP_ID=
EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID=

# Sentry Configuration
SENTRY_DSN=
SENTRY_ENVIRONMENT=development
SENTRY_TRACES_SAMPLE_RATE=1.0
SENTRY_PROFILES_SAMPLE_RATE=1.0

# App Configuration
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_APP_BUILD_NUMBER=1

# Analytics Configuration
EXPO_PUBLIC_ANALYTICS_ENABLED=true
EXPO_PUBLIC_ANALYTICS_SAMPLE_RATE=1.0

# Performance Monitoring
EXPO_PUBLIC_PERFORMANCE_MONITORING_ENABLED=true
EXPO_PUBLIC_PERFORMANCE_SAMPLE_RATE=0.1

# Error Tracking
EXPO_PUBLIC_ERROR_TRACKING_ENABLED=true
EXPO_PUBLIC_ERROR_SAMPLE_RATE=1.0

# Feature Flags
EXPO_PUBLIC_ENABLE_DARK_MODE=true
EXPO_PUBLIC_ENABLE_OFFLINE_SUPPORT=true
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=true

# Google Sign In
GOOGLE_WEB_CLIENT_ID=your_google_web_client_id
GOOGLE_IOS_CLIENT_ID=your_google_ios_client_id
GOOGLE_ANDROID_CLIENT_ID=your_google_android_client_id

# Facebook
FACEBOOK_APP_ID=your_facebook_app_id
FACEBOOK_CLIENT_TOKEN=your_facebook_client_token

# Analytics
APPSFLYER_DEV_KEY=your_appsflyer_dev_key
BRANCH_KEY=your_branch_key

# Weather API
WEATHER_API_KEY=your_weather_api_key
WEATHER_API_URL=https://api.weatherapi.com/v1 